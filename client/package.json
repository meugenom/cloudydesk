{
  "name": "neetcloud",
  "version": "0.1.3",
  "description": "A web-based cloud desktop for users",
  "license": "MIT",
  "private": "false",
  "main": "src/main.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8081",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meugenom/neetcloud "
  },
  "keywords": [
    "cloud-app",
    "neetcloud",
    "web-desktop",
    "web-based",
    "typescript",
    "angular",
    "github"
  ],
  "bugs": {
    "url": "https://github.com/meugenom/neetcloud/issues"
  },
  "homepage": "https://neetcloud.dev",
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ngrx/store-devtools": "^14.3.0",
    "express": "^4.18.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.6",
    "@angular/cli": "~14.0.6",
    "@angular/compiler-cli": "^14.0.0",
    "@ngrx/effects": "^14.3.0",
    "@ngrx/store": "^14.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.1.0",
    "@viselect/vanilla": "^3.0.0",
    "file-saver": "^2.0.5",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "ng2-dragula": "^2.1.1",
    "screenfull": "^6.0.2",
    "typescript": "~4.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
